    async function onConfigChange(config) {
      const baseFont = config.font_family || defaultConfig.font_family;
      const baseFontStack = 'system-ui, -apple-system, sans-serif';
      const customFont = `${baseFont}, ${baseFontStack}`;
      const baseSize = config.font_size || defaultConfig.font_size;

      // Apply colors
      const nav = document.getElementById('main-nav');
      nav.style.backgroundColor = config.primary_action_color || defaultConfig.primary_action_color;

      const mobileMenu = document.getElementById('mobile-menu');
      mobileMenu.style.backgroundColor = config.background_color || defaultConfig.background_color;

      const heroSection = document.getElementById('hero');
      heroSection.style.backgroundColor = config.background_color || defaultConfig.background_color;

      const programsSection = document.getElementById('programs');
      programsSection.style.backgroundColor = config.secondary_color || defaultConfig.secondary_color;

      const footer = document.getElementById('contact');
      footer.style.backgroundColor = config.secondary_action_color || defaultConfig.secondary_action_color;

      const ctaButtons = document.querySelectorAll('.cta-button:not(.cta-button-secondary)');
      ctaButtons.forEach(btn => {
        btn.style.backgroundColor = config.primary_action_color || defaultConfig.primary_action_color;
      });

      // Apply fonts
      document.getElementById('school-name').style.fontFamily = customFont;
      document.getElementById('school-name').style.fontSize = `${baseSize * 1.5}px`;
      
      document.getElementById('page-title').style.fontFamily = customFont;
      document.getElementById('page-title').style.fontSize = `${baseSize * 3.5}px`;
      
      document.getElementById('page-subtitle').style.fontFamily = customFont;
      document.getElementById('page-subtitle').style.fontSize = `${baseSize * 1.25}px`;

      document.querySelectorAll('.program-title').forEach(el => {
        el.style.fontFamily = customFont;
        el.style.fontSize = `${baseSize * 2}px`;
      });

      document.querySelectorAll('.program-description').forEach(el => {
        el.style.fontFamily = customFont;
        el.style.fontSize = `${baseSize * 1.125}px`;
      });

      // Apply text content
      document.getElementById('school-name').textContent = config.school_name || defaultConfig.school_name;
      document.getElementById('page-title').textContent = config.page_title || defaultConfig.page_title;
      document.getElementById('page-subtitle').textContent = config.page_subtitle || defaultConfig.page_subtitle;
      
      document.getElementById('stem-title').textContent = config.stem_title || defaultConfig.stem_title;
      document.getElementById('stem-description').textContent = config.stem_description || defaultConfig.stem_description;
      document.getElementById('stem-subject-1').textContent = config.stem_subject_1 || defaultConfig.stem_subject_1;
      document.getElementById('stem-subject-2').textContent = config.stem_subject_2 || defaultConfig.stem_subject_2;
      document.getElementById('stem-subject-3').textContent = config.stem_subject_3 || defaultConfig.stem_subject_3;
      document.getElementById('stem-subject-4').textContent = config.stem_subject_4 || defaultConfig.stem_subject_4;
      
      document.getElementById('abm-title').textContent = config.abm_title || defaultConfig.abm_title;
      document.getElementById('abm-description').textContent = config.abm_description || defaultConfig.abm_description;
      document.getElementById('abm-subject-1').textContent = config.abm_subject_1 || defaultConfig.abm_subject_1;
      document.getElementById('abm-subject-2').textContent = config.abm_subject_2 || defaultConfig.abm_subject_2;
      document.getElementById('abm-subject-3').textContent = config.abm_subject_3 || defaultConfig.abm_subject_3;
      document.getElementById('abm-subject-4').textContent = config.abm_subject_4 || defaultConfig.abm_subject_4;
      
      document.getElementById('humss-title').textContent = config.humss_title || defaultConfig.humss_title;
      document.getElementById('humss-description').textContent = config.humss_description || defaultConfig.humss_description;
      document.getElementById('humss-subject-1').textContent = config.humss_subject_1 || defaultConfig.humss_subject_1;
      document.getElementById('humss-subject-2').textContent = config.humss_subject_2 || defaultConfig.humss_subject_2;
      document.getElementById('humss-subject-3').textContent = config.humss_subject_3 || defaultConfig.humss_subject_3;
      document.getElementById('humss-subject-4').textContent = config.humss_subject_4 || defaultConfig.humss_subject_4;
      
      document.getElementById('contact-address').textContent = config.contact_address || defaultConfig.contact_address;
      document.getElementById('contact-phone').textContent = config.contact_phone || defaultConfig.contact_phone;
      document.getElementById('contact-email').textContent = config.contact_email || defaultConfig.contact_email;
    }

    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange,
        mapToCapabilities: (config) => ({
          recolorables: [
            {
              get: () => config.background_color || defaultConfig.background_color,
              set: (value) => {
                config.background_color = value;
                window.elementSdk.setConfig({ background_color: value });
              }
            },
            {
              get: () => config.secondary_color || defaultConfig.secondary_color,
              set: (value) => {
                config.secondary_color = value;
                window.elementSdk.setConfig({ secondary_color: value });
              }
            },
            {
              get: () => config.text_color || defaultConfig.text_color,
              set: (value) => {
                config.text_color = value;
                window.elementSdk.setConfig({ text_color: value });
              }
            },
            {
              get: () => config.primary_action_color || defaultConfig.primary_action_color,
              set: (value) => {
                config.primary_action_color = value;
                window.elementSdk.setConfig({ primary_action_color: value });
              }
            },
            {
              get: () => config.secondary_action_color || defaultConfig.secondary_action_color,
              set: (value) => {
                config.secondary_action_color = value;
                window.elementSdk.setConfig({ secondary_action_color: value });
              }
            }
          ],
          borderables: [],
          fontEditable: {
            get: () => config.font_family || defaultConfig.font_family,
            set: (value) => {
              config.font_family = value;
              window.elementSdk.setConfig({ font_family: value });
            }
          },
          fontSizeable: {
            get: () => config.font_size || defaultConfig.font_size,
            set: (value) => {
              config.font_size = value;
              window.elementSdk.setConfig({ font_size: value });
            }
          }
        }),
        mapToEditPanelValues: (config) => new Map([
          ["school_name", config.school_name || defaultConfig.school_name],
          ["page_title", config.page_title || defaultConfig.page_title],
          ["page_subtitle", config.page_subtitle || defaultConfig.page_subtitle],
          ["stem_title", config.stem_title || defaultConfig.stem_title],
          ["stem_description", config.stem_description || defaultConfig.stem_description],
          ["stem_subject_1", config.stem_subject_1 || defaultConfig.stem_subject_1],
          ["stem_subject_2", config.stem_subject_2 || defaultConfig.stem_subject_2],
          ["stem_subject_3", config.stem_subject_3 || defaultConfig.stem_subject_3],
          ["stem_subject_4", config.stem_subject_4 || defaultConfig.stem_subject_4],
          ["abm_title", config.abm_title || defaultConfig.abm_title],
          ["abm_description", config.abm_description || defaultConfig.abm_description],
          ["abm_subject_1", config.abm_subject_1 || defaultConfig.abm_subject_1],
          ["abm_subject_2", config.abm_subject_2 || defaultConfig.abm_subject_2],
          ["abm_subject_3", config.abm_subject_3 || defaultConfig.abm_subject_3],
          ["abm_subject_4", config.abm_subject_4 || defaultConfig.abm_subject_4],
          ["humss_title", config.humss_title || defaultConfig.humss_title],
          ["humss_description", config.humss_description || defaultConfig.humss_description],
          ["humss_subject_1", config.humss_subject_1 || defaultConfig.humss_subject_1],
          ["humss_subject_2", config.humss_subject_2 || defaultConfig.humss_subject_2],
          ["humss_subject_3", config.humss_subject_3 || defaultConfig.humss_subject_3],
          ["humss_subject_4", config.humss_subject_4 || defaultConfig.humss_subject_4],
          ["contact_address", config.contact_address || defaultConfig.contact_address],
          ["contact_phone", config.contact_phone || defaultConfig.contact_phone],
          ["contact_email", config.contact_email || defaultConfig.contact_email]
        ])
      });
    }
